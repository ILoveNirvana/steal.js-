{
  "name": "Steal Js",
  "description": "For 1-st level websites",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["tinymce/tinymce.min.js", "FileSaver.min.js", "jszip.js", "axios.js", "main.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Steal it!"
  },
  "manifest_version": 2
}